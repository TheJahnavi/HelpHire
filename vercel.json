{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist/public"
      }
    },
    {
      "src": "server/vercel-entry.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/vercel-entry.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel-entry.ts"
    }
  ]
}