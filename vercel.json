{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist/public",
        "distDir": "dist/public",
        "buildCommand": "npm run vercel-build"
      }
    },
    {
      "src": "server/vercel-handler.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "dist/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/vercel-handler.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel-entry.js"
    }
  ]
}