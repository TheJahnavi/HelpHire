{
  "version": 2,
  "builds": [
    {
      "src": "dist/public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "server/api-handler.ts",
      "use": "@vercel/node"
    },
    {
      "src": "server/vercel-entry.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/api-handler.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel-entry.ts"
    }
  ]
  // Updated to fix import issues with routes.vercel.ts
}