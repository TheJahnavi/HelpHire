{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist/public",
        "distDir": "dist/public"
      }
    },
    {
      "src": "server/routes.vercel.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "{shared/**,server/**,package.json}",
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "server/vercel-entry.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "dist/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/routes.vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel-entry.ts"
    }
  ]
}