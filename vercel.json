{
  "version": 2,
  "builds": [
    {
      "src": "server/vercel-entry.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "dist/public/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/api-handler.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel-entry.ts"
    }
  ]
}