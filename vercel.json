{
  "version": 2,
  "builds": [
    {
      "src": "server/vercel.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**", "server/routes.vercel.ts"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel.ts"
    }
  ]
}